{
  "name": "api",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "inputs": ["{projectRoot}/**/*", "!{projectRoot}/_build/**/*"],
      "outputs": ["{projectRoot}/_build"],
      "dependsOn": ["^build"],
      "options": {
        "commands": ["gulp build"],
        "cwd": "api"
      }
    },
    "dev": {
      "executor": "nx:run-script",
      "options": {
        "script": "dev"
      },
      "dependsOn": ["^dev"]
    },
    "is-docker-published-api": {
      "executor": "workspace-plugin:is-docker-published",
      "options": {
        "packageJson": "api/package.json",
        "registry": "registry.cn-hongkong.aliyuncs.com",
        "imageName": "merico/table"
      }
    },
    "is-docker-published-website": {
      "executor": "workspace-plugin:is-docker-published",
      "options": {
        "packageJson": "api/package.json",
        "registry": "registry.cn-hongkong.aliyuncs.com",
        "imageName": "merico/table-website"
      }
    }
  }
}
